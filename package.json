{
  "name": "@cynerad/support",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.10.0",
  "description": "just skeleton for my typescript library",
  "author": "your name",
  "license": "MIT",
  "keywords": [],
  "exports": {
    "./string": {
      "types": "./dist/string/index.d.ts",
      "import": "./dist/string/index.js",
      "require": "./dist/string/index.cjs"
    },
    "./number": {
      "types": "./dist/number/index.d.ts",
      "import": "./dist/number/index.js",
      "require": "./dist/number/index.cjs"
    }
  },
  "files": [
    "dist"
