    "shadcn": "^3.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*": "pnpm lint"
